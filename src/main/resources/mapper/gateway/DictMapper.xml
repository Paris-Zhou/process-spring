<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paris.mapper.gateway.DictMapper">
    <select id="selectOne" resultType="com.paris.pojo.Dict">
        select *
        from iot_dict
        where dict_id = #{dictId}
    </select>
    <select id="selectList" resultType="com.paris.pojo.Dict">
        select *
        from iot_dict
        where dict_parent_id = #{parentDictId}
    </select>
</mapper>